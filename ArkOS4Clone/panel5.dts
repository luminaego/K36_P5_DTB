&display_subsystem {
	status = "okay";
};

&dsi {
	status = "okay";

	panel@0 {
		compatible = "simple-panel-dsi";
		reg = <0>;
		/* pwm1 backlight */
		backlight = <&backlight>;
		/* RK817 LDO7 */
		backlight-supply = <&vcc_backlight>;
		/* RK817 LDO8 */

		power-supply = <&vcc18_lcd0>;
		/* LCD reset gpio GPIO3.D3 */
	        reset-gpios = <&gpio3 RK_PD3 GPIO_ACTIVE_LOW>;
			enable-gpios = <&gpio3 RK_PA3 GPIO_ACTIVE_HIGH>;
