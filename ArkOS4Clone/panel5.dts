		drive-strength-s = <0x06>;
	};

	pcfg_pull_none_8_6ma: pcfg-pull-none-8-6ma {
		bias-disable;
		drive-strength = <0x08>;
		drive-strength-s = <0x06>;
	};

	emmc {
        emmc_clk: emmc-clk {
            rockchip,pins = <1 RK_PB1 0x02 &pcfg_pull_none_8_6ma>;
        };
        emmc_cmd: emmc-cmd {
            rockchip,pins = <1 RK_PB2 0x02 &pcfg_pull_up_8_6ma>;
        };
        emmc_bus8: emmc-bus8 {
            rockchip,pins = 
			<1 RK_PA0 0x02 &pcfg_pull_up_8_6ma>,
			<1 RK_PA1 0x02 &pcfg_pull_up_8_6ma>,
			<1 RK_PA2 0x02 &pcfg_pull_up_8_6ma>,
			<1 RK_PA3 0x02 &pcfg_pull_up_8_6ma>,
			<1 RK_PA4 0x02 &pcfg_pull_up_8_6ma>,
			<1 RK_PA5 0x02 &pcfg_pull_up_8_6ma>,
			<1 RK_PA6 0x02 &pcfg_pull_up_8_6ma>,
			<1 RK_PA7 0x02 &pcfg_pull_up_8_6ma>;
        };
        emmc_pwren: emmc-pwren {
            rockchip,pins = <1 RK_PB0 0x02 &pcfg_pull_none>;
        };
        emmc_rstnout: emmc-rstnout {
            rockchip,pins = <1 RK_PB3 0x02 &pcfg_pull_none>;
        };
    };

	pmic {
		/* ODROID-GO2 : PMIC_INT(GPIO0.PB2) */
		pmic_int: pmic_int {
			rockchip,pins =
				<0 RK_PB2 RK_FUNC_GPIO &pcfg_pull_up>;
				// <0 RK_PB3 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		/* ODROID-GO2 : PMIC_SLEEP(GPIO0.PA4) */
		soc_slppin_gpio: soc_slppin_gpio {
			rockchip,pins =
				<0 RK_PA4 RK_FUNC_GPIO &pcfg_output_low>;
		};

		soc_slppin_slp: soc_slppin_slp {
			rockchip,pins =
				<0 RK_PA4 RK_FUNC_1 &pcfg_pull_none>;
		};

		soc_slppin_rst: soc_slppin_rst {
			rockchip,pins =
				<0 RK_PA4 RK_FUNC_2 &pcfg_pull_none>;
		};
	};
    vcc18-lcd {
